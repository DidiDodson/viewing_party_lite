<h1>Top Rated Movies</h1>

<%= button_to "Discover Page", "/users/#{@user.id}/discover", method: :get %>

<div id="topmovies">
  <% @movies.each do |movie| %>
      <%= link_to movie[:original_title], "/users/#{@user.id}/movies/#{movie[:id]}" %> Vote Average: <%= movie[:vote_average] %><br>
  <% end %>
</div>
