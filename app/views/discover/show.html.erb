<h1>Discover Movies</h1>

<%= button_to "Find Top Rated Movies", movie_index_path(@user), method: :get %><br>

<%= form_with url: movie_index_path(@user), method: :get, local: true do |form| %>
  <%= form.text_field :original_title, placeholder: "Search by movie title" %>
  <%= form.submit "Find Movies"%>
<% end %>
